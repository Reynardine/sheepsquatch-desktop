#: sheep/sheep.c:532
msgid "Syntax: sheep [options]\n"
msgstr "Syntaxe : sheep [options]\n"

#: sheep/sheep.c:533
msgid ""
"\n"
"Options:\n"
msgstr ""
"\n"
"Options :\n"

#: sheep/sheep.c:534
msgid "  --help              Display this help and exit\n"
msgstr "  --help              Affiche cette aide et quitte\n"

#: sheep/sheep.c:535
msgid "  --version           Display the version and exit\n"
msgstr "  --version           Affiche la version et quitte\n"

#: sheep/sheep.c:536
msgid "  --replace           Replace the currently running window manager\n"
msgstr ""
"  --replace           Remplace le gestionnaire de fenêtres actuellement en "
"usage\n"

#. TRANSLATORS: if you translate "FILE" here, make sure to keep the "Specify..."
#. aligned still, if you have to, make a new line with \n and 22 spaces. It's
#. fine to leave it as FILE though.
#: sheep/sheep.c:540
msgid "  --config-file FILE  Specify the path to the config file to use\n"
msgstr ""
"  --config-file FILE  Spécifie le chemin du fichier de configuration à "
"utiliser\n"

#: sheep/sheep.c:541
msgid "  --sm-disable        Disable connection to the session manager\n"
msgstr ""
"  --sm-disable        Désactive la connexion au gestionnaire de sessions\n"

#: sheep/sheep.c:542
msgid ""
"\n"
"Passing messages to a running Openbox instance:\n"
msgstr ""
"\n"
"Passage de messages à l'instance d'Openbox en cours :\n"

#: sheep/sheep.c:543
msgid "  --reconfigure       Reload Openbox's configuration\n"
msgstr "  --reconfigure       Recharge la configuration d'Openbox\n"

#: sheep/sheep.c:544
msgid "  --restart           Restart Openbox\n"
msgstr "  --restart           Redémarre Openbox\n"

#: sheep/sheep.c:545
msgid "  --exit              Exit Openbox\n"
msgstr "  --exit              Sortir d'Openbox\n"

#: sheep/sheep.c:546
msgid ""
"\n"
"Debugging options:\n"
msgstr ""
"\n"
"Options de déboguage :\n"

#: sheep/sheep.c:547
msgid "  --sync              Run in synchronous mode\n"
msgstr "  --sync              Exécute en mode synchrone\n"

#: sheep/sheep.c:548
msgid "  --startup CMD       Run CMD after starting\n"
msgstr ""

#: sheep/sheep.c:549
msgid "  --debug             Display debugging output\n"
msgstr "  --debug             Affiche la sortie de déboguage\n"

#: sheep/sheep.c:550
msgid "  --debug-focus       Display debugging output for focus handling\n"
msgstr ""
"  --debug-focus       Affiche la sortie de déboguage pour la gestion du "
"focus\n"

#: sheep/sheep.c:551
msgid "  --debug-session     Display debugging output for session management\n"
msgstr ""
"  --debug-session     Affiche la sortie de déboguage pour la gestion du "
"session\n"

#: sheep/sheep.c:552
msgid "  --debug-xinerama    Split the display into fake xinerama screens\n"
msgstr ""
"  --debug-xinerama    Découpe l'affichage en écrans xinerama factices\n"

