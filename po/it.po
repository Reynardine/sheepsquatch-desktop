#: sheep/sheep.c:533
msgid ""
"\n"
"Options:\n"
msgstr ""
"\n"
"Opzioni:\n"

#: sheep/sheep.c:534
msgid "  --help              Display this help and exit\n"
msgstr "  --help              Mostra questo messaggio di aiuto ed esce\n"

#: sheep/sheep.c:535
msgid "  --version           Display the version and exit\n"
msgstr "  --version           Mostra il numero di versione ed esce\n"

#: sheep/sheep.c:536
msgid "  --replace           Replace the currently running window manager\n"
msgstr "  --replace           Sostituisce il gestore di finestre attivo\n"

#. TRANSLATORS: if you translate "FILE" here, make sure to keep the "Specify..."
#. aligned still, if you have to, make a new line with \n and 22 spaces. It's
#. fine to leave it as FILE though.
#: sheep/sheep.c:540
msgid "  --config-file FILE  Specify the path to the config file to use\n"
msgstr ""
"  --config-file FILE  Specifica il percorso del file di configurazione\n"

#: sheep/sheep.c:541
msgid "  --sm-disable        Disable connection to the session manager\n"
msgstr "  --sm-disable        Disabilita connessione al gestore di sessione\n"

#: sheep/sheep.c:542
msgid ""
"\n"
"Passing messages to a running Openbox instance:\n"
msgstr ""
"\n"
"Inviare messaggi ad un'istanza di Openbox attiva:\n"

#: sheep/sheep.c:543
msgid "  --reconfigure       Reload Openbox's configuration\n"
msgstr "  --reconfigure       Ricarica la configurazione di Openbox\n"

#: sheep/sheep.c:544
msgid "  --restart           Restart Openbox\n"
msgstr "  --restart           Riavvia Openbox\n"

#: sheep/sheep.c:545
msgid "  --exit              Exit Openbox\n"
msgstr "  --exit              Termina Openbox\n"

#: sheep/sheep.c:546
msgid ""
"\n"
"Debugging options:\n"
msgstr ""
"\n"
"Opzioni di debug:\n"

#: sheep/sheep.c:547
msgid "  --sync              Run in synchronous mode\n"
msgstr "  --sync              Esegue in modalità sincrona\n"

#: sheep/sheep.c:548
msgid "  --startup CMD       Run CMD after starting\n"
msgstr ""

#: sheep/sheep.c:549
msgid "  --debug             Display debugging output\n"
msgstr "  --debug             Mostra le informazioni di debug\n"

#: sheep/sheep.c:550
msgid "  --debug-focus       Display debugging output for focus handling\n"
msgstr ""
"  --debug-focus       Mostra le informazioni di debug sulla gestione del "
"focus\n"

#: sheep/sheep.c:551
msgid "  --debug-session     Display debugging output for session management\n"
msgstr ""

#: sheep/sheep.c:552
msgid "  --debug-xinerama    Split the display into fake xinerama screens\n"
msgstr "  --debug-xinerama    Divide lo schermo per simulare xinerama\n"

