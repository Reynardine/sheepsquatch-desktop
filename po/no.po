#. aligned still, if you have to, make a new line with \n and 22 spaces. It's
#. fine to leave it as FILE though.
#: sheep/sheep.c:540
msgid "  --config-file FILE  Specify the path to the config file to use\n"
msgstr ""
"  --config-file FIL   Spesifisér filbane til konfigurasjonsfilen du vil ta i "
"bruk\n"

#: sheep/sheep.c:541
msgid "  --sm-disable        Disable connection to the session manager\n"
msgstr "  --sm-disable        Deaktiver tilknytning til sesjonsbehandleren\n"

#: sheep/sheep.c:542
msgid ""
"\n"
"Passing messages to a running Openbox instance:\n"
msgstr ""
"\n"
"Sender beskjeder til en kjørende Openbox-instans:\n"

#: sheep/sheep.c:543
msgid "  --reconfigure       Reload Openbox's configuration\n"
msgstr "  --reconfigure       Oppdater Openbox' konfigurasjon\n"

#: sheep/sheep.c:544
msgid "  --restart           Restart Openbox\n"
msgstr "  --restart           Start Openbox på nytt\n"

#: sheep/sheep.c:545
msgid "  --exit              Exit Openbox\n"
msgstr "  --exit              Avslutt Openbox\n"

#: sheep/sheep.c:546
msgid ""
"\n"
"Debugging options:\n"
msgstr ""
"\n"
"Debug-alternativ:\n"

#: sheep/sheep.c:547
msgid "  --sync              Run in synchronous mode\n"
msgstr "  --sync              Kjør i synkron-modus\n"

#: sheep/sheep.c:548
msgid "  --startup CMD       Run CMD after starting\n"
msgstr "  --startup CMD       Kjør CMD (kommando) etter oppstart\n"

#: sheep/sheep.c:549
msgid "  --debug             Display debugging output\n"
msgstr "  --debug             Vis debuggingsinformasjon\n"

#: sheep/sheep.c:550
msgid "  --debug-focus       Display debugging output for focus handling\n"
msgstr "  --debug-focus       Vis debuggingsinformasjon for fokus-håndtering\n"

#: sheep/sheep.c:551
msgid "  --debug-session     Display debugging output for session management\n"
msgstr ""
"  --debug-session     Vis debuggingsinformasjon for session-håndtering\n"

#: sheep/sheep.c:552
msgid "  --debug-xinerama    Split the display into fake xinerama screens\n"
msgstr "  -debug-xinerama    Splitt displayet for falske xinerama-skjermer\n"

