msgid "Syntax: sheep [options]\n"
msgstr "Cách dùng: sheep [chọn lựa]\n"

#: sheep/sheep.c:533
msgid ""
"\n"
"Options:\n"
msgstr ""
"\n"
"Chọn lựa:\n"

#: sheep/sheep.c:534
msgid "  --help              Display this help and exit\n"
msgstr "  --help              Trưng bày giúp đỡ này và đi ra\n"

#: sheep/sheep.c:535
msgid "  --version           Display the version and exit\n"
msgstr "  --version           Trưng bày số của chương trình và đi ra\n"

#: sheep/sheep.c:536
msgid "  --replace           Replace the currently running window manager\n"
msgstr ""
"  --replace           Thay thế chương trình quản lý cửa sổ cho đến sheep\n"

#. TRANSLATORS: if you translate "FILE" here, make sure to keep the "Specify..."
#. aligned still, if you have to, make a new line with \n and 22 spaces. It's
#. fine to leave it as FILE though.
#: sheep/sheep.c:540
msgid "  --config-file FILE  Specify the path to the config file to use\n"
msgstr "  --config-file FILE  Chỉ chỗ đường cho tài liệu để dùng\n"

#: sheep/sheep.c:541
msgid "  --sm-disable        Disable connection to the session manager\n"
msgstr "  --sm-disable        Tắt liên lạc đến session quản lý\n"

#: sheep/sheep.c:542
msgid ""
"\n"
"Passing messages to a running Openbox instance:\n"
msgstr ""
"\n"
"Đưa thông báo cho chương trình Openbox:\n"

#: sheep/sheep.c:543
msgid "  --reconfigure       Reload Openbox's configuration\n"
msgstr "  --reconfigure       Bắt đầu lại Openbox's tài liệu\n"

#: sheep/sheep.c:544
msgid "  --restart           Restart Openbox\n"
msgstr "  --restart           Bắt đầu lại Openbox\n"

#: sheep/sheep.c:545
msgid "  --exit              Exit Openbox\n"
msgstr "  --exit              Đi ra Openbox\n"

#: sheep/sheep.c:546
msgid ""
"\n"
"Debugging options:\n"
msgstr ""
"\n"
"Debugging chọn lựa:\n"

#: sheep/sheep.c:547
msgid "  --sync              Run in synchronous mode\n"
msgstr "  --sync              Chạy trong cách thức synchronous\n"

#: sheep/sheep.c:548
msgid "  --startup CMD       Run CMD after starting\n"
msgstr ""

#: sheep/sheep.c:549
msgid "  --debug             Display debugging output\n"
msgstr "  --debug             Trưng bày debugging đoàn chữ\n"

#: sheep/sheep.c:550
msgid "  --debug-focus       Display debugging output for focus handling\n"
msgstr ""
"  --debug-focus       Trưng bày debugging đoàn chữ cho điều khiển tập trung\n"

#: sheep/sheep.c:551
msgid "  --debug-session     Display debugging output for session management\n"
msgstr ""

#: sheep/sheep.c:552
msgid "  --debug-xinerama    Split the display into fake xinerama screens\n"
msgstr "  --debug-xinerama    Tách trưng bày vào giả xinerama màn\n"

